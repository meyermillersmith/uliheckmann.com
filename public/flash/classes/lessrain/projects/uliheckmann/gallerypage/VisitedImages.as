/** * @author Thomas Meyer, Less Rain (thomas@lessrain.com) */class lessrain.projects.uliheckmann.gallerypage.VisitedImages{		private static var instance : VisitedImages;	private var _visitedImageIDs : Array;		/**	 * @return singleton instance of VisitedImages	 */	public static function getInstance() : VisitedImages	{		if (instance == null) instance = new VisitedImages();		return instance;	}		private function VisitedImages()	{		_visitedImageIDs = new Array();	}		public function isVisited(id_:String):Boolean	{		for (var i : Number = _visitedImageIDs.length-1; i >= 0; i--) if (_visitedImageIDs[i]==id_) return true;	}		public function setVisited(id_:String):Void	{		if (!isVisited(id_)) _visitedImageIDs.push(id_);	}	}
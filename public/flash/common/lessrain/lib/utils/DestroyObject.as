import flash.display.BitmapData;import lessrain.lib.utils.MovieClipUtils; /** * @author max kugland *  * @usage: * var dObj:DestroyObject=new DestroyObject(); * dObj.destroy(this); * dObj.show(this, "extendedPage"); * delete dObj; */class lessrain.lib.utils.DestroyObject{        public function DestroyObject()    {	    }        public function destroy(obj:Object) :Void    {		for (var i:String in obj)		{			if (obj[i] instanceof BitmapData) BitmapData( obj[i] ).dispose();			else if (typeof obj[i]=="movieclip") obj[i].removeMovieClip();			else if (typeof obj[i]=="boolean") obj[i]=false;			else if (typeof obj[i]=="number") obj[i]=0;			delete obj[i];						// If still NOT deleted, try KillerBeacon			if (obj[i]!=null )			{				MovieClipUtils.killerBeacon( MovieClip(obj[i]) );			}		}				// The object itself		if (typeof obj=="movieclip") obj.removeMovieClip();		delete obj;				// If still NOT deleted, try KillerBeacon		if (obj!=null) MovieClipUtils.killerBeacon( MovieClip(obj) );		delete obj;    }        public function show(obj:Object, name:String):Void    {    	trace("//////////////////// "+name+" ///////////////////////");    	_global.ASSetPropFlags (obj, null, 8, 1);    	var out:String="";     	for (var i :String in obj)      		out += i + " : " + obj[i] + " : " + typeof obj[i] + "\n";      	trace(out);    			    }}